{
  "name": "react-scene",
  "version": "0.1.7",
  "author": {
    "name": "Folklore",
    "email": "info@atelierfolklore.ca"
  },
  "contributors": [
    {
      "name": "David Mongeau-Petitpas",
      "email": "dmp@atelierfolklore.ca"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Folkloreatelier/react-scene.git"
  },
  "main": "dist/react-scene.js",
  "scripts": {
    "clean:dist": "rm -rf ./dist",
    "clean:tmp": "rm -rf ./.tmp",
    "clean": "npm run clean:tmp && npm run clean:dist",
    "mkdir:dist": "mkdir -p ./dist",
    "mkdir:tmp": "mkdir -p ./.tmp",
    "mkdir": "npm run mkdir:tmp && npm run mkdir:dist",
    "build:files": "npm run clean && npm run mkdir",
    "build": "npm run build:files && npm run build:js && npm run clean:tmp",
    "watch": "npm run clean:tmp && npm run mkdir:tmp",
    "browsersync": "node build/browsersync.js",
    "server": "concurrently \"npm run watch\" \"npm run browsersync\"",
    "test": "mocha",
    "jshint:dist": "jshint src/**.js",
    "jshint": "npm run jshint:dist",
    "jscs": "jscs src/**.js",
    "webpack:dist": "webpack --config build/webpack.config.build.js",
    "webpack:min": "webpack --config build/webpack.config.min.js",
    "webpack": "npm run webpack:dist && npm run webpack:min",
    "scripts:dist": "npm run webpack",
    "scripts": "npm run scripts:dist",
    "build:js": "npm run jshint && npm run scripts",
    "release": "build/release.sh"
  },
  "dependencies": {
    "hoist-non-react-statics": "^1.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "brfs": "^1.4.3",
    "browser-sync": "^2.15.0",
    "bs-fullscreen-message": "^1.1.0",
    "commander": "^2.9.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.4",
    "customizr": "^1.0.0-alpha",
    "expose-loader": "^0.7.1",
    "html-loader": "^0.4.3",
    "imagemin-cli": "^3.0.0",
    "imports-loader": "^0.6.5",
    "jshint": "^2.9.3",
    "json-loader": "^0.5.4",
    "postcss-cli": "^2.6.0",
    "proxy-middleware": "^0.15.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "serve-static": "^1.11.1",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "svg-react-loader": "^0.3.7",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1"
  }
}
